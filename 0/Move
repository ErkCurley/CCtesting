local modem = peripheral.wrap("left")


function forward(num)
  for i=0,num do 
    turtle.forward()
  end
end

function back(num)
  turtle.turnLeft()
  turtle.turnLeft()
  for i=0,num do 
    turtle.forward()
  end
end

function right(num)
  turtle.turnRight()
  for i=0,num do 
    turtle.forward()
  end
end
function left(num)
  turtle.turnLeft()
  for i=0,num do 
    turtle.forward()
  end
end

local x, y, z = gps.locate(5)

print(x)
